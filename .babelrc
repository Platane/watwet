{
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src",
          "type": "./src/type"
        }
      }
    ],
    [
      "transform-inline-environment-variables",
      {
        "include": [
          "NODE_ENV",
          "VERSION",
          "PATHNAME_BASE",
          "SENTRY_DSN",
          "HOST"
        ]
      }
    ],
    "transform-class-properties",
    "transform-object-rest-spread",
    "emotion",
    ["transform-react-jsx", { "pragma": "h" }]
  ],
  "presets": ["flow"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
